library(psych)
oqhWOaAcvzUN7pOq
getwd()
setwd("C:/Users/andresfelipe/Documents/Biología/Estadistica/R lab")
datos<-read.csv("2007_EAA_Magud_et_al_data.csv")
View(datos)
?datos
mean(datos$A)
mean(datos$P)
mean(datos$W)
median(datos$A)
median(datos$P)
median(datos$W)
moda<-function(n){
  x<-table (n)
  mode<-x[which.max(x)]
  return(mode)
}##pero no es necesario por ser variable numerica  continua
tapply(datos$A,INDEX = datos$Traits,FUN = summary)
tapply(datos$P,INDEX = datos$Traits,FUN = summary)
tapply(datos$W,INDEX = datos$Traits,FUN = summary)
var(datos$A)
var(datos$P)
var(datos$W)
sd(datos$A)
sd(datos$P)
sd(datos$W)
describe.by(datos[,c("A","P","W")],group =datos$Traits)
Belg_A<-hist(datos$A[which(datos$Traits==c("I BK","II BK"))],main= "histograma de longitud del cuerpo de eriófidos de Belgrado",xlab="Longitud",ylab="Frecuencia")
Ivan_A<-hist(datos$A[which(datos$Traits==c("I IV","II IV"))],main= "histograma de longitud del cuerpo de eriófidos de Vianca",xlab="Longitud",ylab="Frecuencia")
Belg_P<-hist(datos$P[which(datos$Traits==c("I BK","II BK"))],main= "histograma de tubercles 1b apart  de eriófidos de Belgrado",xlab="Longitud",ylab="Frecuencia")
Ivan_P<-hist(datos$P[which(datos$Traits==c("I IV","II IV"))],main= "histograma de tubercles 1b apart  de eriófidos de  Vianca",xlab="Longitud",ylab="Frecuencia")
Belg_W<-hist(datos$W[which(datos$Traits==c("I BK","II BK"))],main= "histograma de tarsus I length  de eriófidos de Belgrado",xlab="Longitud",ylab="Frecuencia")
Ivan_W<-hist(datos$W[which(datos$Traits==c("I IV","II IV"))],main= "histograma de tarsus I length de eriófidos de  Vianca",xlab="Longitud",ylab="Frecuencia")
boxplot_Belg_A<-boxplot(datos$A[which(datos$Traits==c("I BK","II BK"))],notch=T,main="Longitud del cuerpo de eriófidos de Belgrado")
boxplot_Ivan_A<-boxplot(datos$A[which(datos$Traits==c("I IV","II IV"))],notch=T,main="Longitud del cuerpo de eriófidos de Vianca")
boxplot_Belg_P<-boxplot(datos$P[which(datos$Traits==c("I BK","II BK"))],notch=T,main="tubercles 1b apart  de eriófidos de Belgrado")
boxplot_Ivan_P<-boxplot(datos$P[which(datos$Traits==c("I IV","II IV"))],notch=T,main="tubercles 1b apart  de eriófidos de Ivanca")
boxplot_Belg_W<-boxplot(datos$W[which(datos$Traits==c("I BK","II BK"))],notch=T,main="Tarsus I length de eriófidos de  Belgrado")
boxplot_Ivan_W<-boxplot(datos$W[which(datos$Traits==c("I IV","II IV"))],notch=T,main="Tarsus I length de eriófidos de  Vianca")
png("Belg_A")
##Al analizar los histogramas y los boxplot de las diferentes caracteristicas de ambas localidades se puede inferir que hay diferencias  debido a que los estadisticos difieren significativamente entre las dos localidades.
